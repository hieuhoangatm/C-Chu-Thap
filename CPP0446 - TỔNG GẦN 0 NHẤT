#include<bits/stdc++.h>

using namespace std;

main(){
	int t; cin >> t;
	while(t--){
		int n; cin >> n;
		int a[n];
		for(int i=0;i<n;i++) cin >> a[i];
		int max=10e9,tmp;
		for(int i=0;i<n;i++){
			int s=10e5;
			for(int j=i+1;j<n;j++){
				if(abs(a[i]+a[j])<max){
					max=abs(a[i]+a[j]);
					tmp=a[i]+a[j];
			}
			}
		}
		cout << tmp<< endl;
	} 
} 
