#include<bits/stdc++.h> 
using namespace std;

int main(){ 
	int t; cin >> t; 
	while(t--){ 
		int n,m,l,r; 
		cin >> n >> m; 
		int a[n]; 
		for(int i=1;i<=n;i++) cin >> a[i]; 
		while(m--){ 
			long long sum=0; 
			cin >> l >> r; 
			for(int i=l;i<=r;i++)  sum+=a[i]; 
			cout << sum << endl; 
		} 
	}  
}
