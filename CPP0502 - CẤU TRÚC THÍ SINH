#include<bits/stdc++.h>
using namespace std;
using ll =long long;
struct ThiSinh{
	string hoten, ngaysinh;
	double diem1,diem2,diem3,tong;
};

void nhap(ThiSinh &p){
	getline(cin, p.hoten);
	cin >> p.ngaysinh;
	cin.ignore();
	cin >> p.diem1 >> p.diem2 >> p.diem3;
	p.tong=p.diem1+ p.diem2 + p.diem3;
}
void in(ThiSinh p){
	cout << p.hoten << " " << p.ngaysinh << " " << fixed << setprecision(1)<< p.tong << endl;
}
int main(){
    struct ThiSinh A;
    nhap(A);
    in(A);
    return 0;
}
