#include<bits/stdc++.h>

using namespace std;

void vietthuong(string &x){
	for(int i=0;i<x.length();i++) x[i]=tolower(x[i]);
}

main(){
	string s; cin >> s;
	vietthuong(s);
	for(int i=0;i<s.length();i++){
		if(s[i]!='a' && s[i] != 'e' && s[i] != 'i' && s[i] != 'o' && s[i] !='u' && s[i]!='y' ) cout << "." << s[i];
	}
}
