#include<iostream>
#include<string>
#include<ctype.h>
#include<bits/stdc++.h>

using namespace std;

main(){
	int t; cin >> t;
	cin.ignore();
	while(t--){
		char a[10000]; cin >> a;
		int ok=1;
		for(int i=0;i<strlen(a);i++){
			if(a[i]!='0' && a[i]!='8' && a[i]!='6') ok=0;
		}
		if(ok==1) cout << "YES\n";
		else cout << "NO\n";
    }
}
