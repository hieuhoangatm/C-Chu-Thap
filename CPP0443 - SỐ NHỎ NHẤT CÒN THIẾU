#include<bits/stdc++.h>

using namespace std;

main(){
	int t; cin >> t;
	while(t--){
		int n; cin >> n;
		int a[n-1];
		//map<int,bool> mp;
		for(int i=0;i<n-1;i++){
			cin >> a[i];
	    }
	    for(int i=1;i<n-1;i++){
	    	if(a[i]-a[i-1]!=1){
	    		cout << a[i-1]+1 << endl;
	    		break;
	        }
		}
		
	}
}
