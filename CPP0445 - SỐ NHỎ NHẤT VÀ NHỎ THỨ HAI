#include<bits/stdc++.h>

using namespace std;

main(){
	int t; cin >> t;
	while(t--){
		int n; cin >> n;
		int ok=1;
		int a[n];
		for(int i=0;i<n;i++) cin >> a[i];
		sort(a,a+n);
		//cout << a[0] << " " << a[1]; 
		if(a[0]>=a[1]) ok=0;
		if(ok) cout << a[0] << " " << a[1]  << endl;
		else cout << "-1\n"; 
	}
}
